{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "checkJs": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "alwaysStrict": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "pretty": true
  }
}
