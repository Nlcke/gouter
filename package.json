{
  "name": "gouter",
  "version": "0.6.1",
  "description": "universal router",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./native": "./native/index.js",
    "./navigators": "./navigators/index.js"
  },
  "dependencies": {
    "path-to-regexp": "6.2.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.8.2",
    "typedoc": "^0.23.24"
  },
  "peerDependencies": {
    "history": "^5.3.0"
  },
  "scripts": {
    "prepack": "tsc index.js native/index.js navigators/index.js --declaration --allowJs --emitDeclarationOnly --skipLibCheck",
    "postpack": "rimraf index.d.ts native/index.d.ts navigators/index.d.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "index.js",
    "native/index.js",
    "navigators/index.js",
    "*.d.ts"
  ],
  "keywords": [
    "web",
    "react",
    "react-native",
    "router"
  ],
  "author": "N1cke",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nlcke/gouter.git"
  }
}
