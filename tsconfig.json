{
  "compilerOptions": {
    "lib": ["ES2015"],
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["index.js", "navigators/index.js", "native/index.js"],
  "typedocOptions": {
    "entryPoints": ["index.js", "navigators/index.js", "native/index.js"],
    "out": "docs",
    "media": "media"
  }
}
