{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "lib": ["ES2015"],
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "index.js",
    "state/index.js",
    "navigators/index.js",
    "native/index.js",
    "linking/index.js"
  ],
  "typedocOptions": {
    "entryPoints": [
      "index.js",
      "state/index.js",
      "navigators/index.js",
      "native/index.js",
      "linking/index.js"
    ],
    "out": "docs",
    "media": "media"
  }
}
